#include <stdio.h>
#include <stdlib.h>

int main()
{
    char myName[] = "Ezequiel";
    int myAge = 21;
    char myCountry[] = "Brazil";
    char futureJob[] = "programmer";

    printf("WHO AM I? \n");
    printf("What's your name? \n");
    printf("My name is %s \n", myName);
    printf("How many years old? \n");
    printf("And i have %d years old \n", myAge);
    printf("Where you from? \n");
    printf("I'm from %s \n", myCountry);
    printf("What have you been study? \n");
    printf("I have study to become a %s \n", futureJob);
    return 0;
}
